{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "dom.iterable", "es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "declaration": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "src/**/*",
    "src/**/*.d.ts",
    "gatsby-config.ts",
    "gatsby-node.ts",
    "gatsby-browser.ts",
    "gatsby-ssr.ts"
  ],
  "exclude": [
    "node_modules",
    "public",
    ".cache"
  ]
}